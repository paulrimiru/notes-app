{
  "version": "0.2.0",
  "configurations": [{
    "name": "Firefox Debugger",
    "type": "firefox",
    "request": "launch",
    "reAttach": true,
    "url": "http://localhost:3000",
    "webRoot": "${workspaceRoot}/src",
    "sourceMapPathOverrides": {
      "webpack:///src/*": "${webRoot}/*"
    }
  }]
}